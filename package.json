{
  "name": "peculiar-certificates-viewer-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "certificates-viewer",
    "certificate-viewer",
    "certificate",
    "x509",
    "decoding",
    "web-component",
    "webpki",
    "pki",
    "asn1"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "clean": "lerna run --parallel clean",
    "lint": "eslint",
    "dev": "npm run --workspace @peculiar/certificates-viewer dev",
    "prepare": "npm run build",
    "publish:version": "lerna publish",
    "publish:canary": "lerna publish --canary"
  },
  "devDependencies": {
    "@peculiar/eslint-config-base": "^0.2.4",
    "@peculiar/eslint-config-react": "^0.2.2",
    "lerna": "^9.0.4"
  },
  "license": "MIT",
  "author": "PeculiarVentures Team",
  "homepage": "https://x509.io/",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=24",
      "onFail": "warn"
    },
    "packageManager": {
      "name": "npm",
      "version": ">=10",
      "onFail": "error"
    }
  }
}
