{
  "name": "peculiar-certificates-viewer-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "certificates-viewer",
    "certificate-viewer",
    "certificate",
    "x509",
    "decoding",
    "web-component",
    "webpki",
    "pki",
    "asn1"
  ],
  "scripts": {
    "build": "lerna run build",
    "test": "lerna run test",
    "clean": "lerna run --parallel clean",
    "lint": "eslint",
    "bootstrap": "yarn",
    "bootstrap:undo": "lerna clean --yes",
    "bootstrap:upgrade": "yarn upgrade-interactive --latest",
    "start:webcomponents": "yarn --cwd packages/webcomponents start",
    "prepare": "yarn build",
    "publish:version": "lerna publish",
    "publish:canary": "lerna publish --canary"
  },
  "devDependencies": {
    "@peculiar/eslint-config-base": "^0.2.4",
    "@peculiar/eslint-config-react": "^0.2.2",
    "lerna": "^8.2.1"
  },
  "license": "MIT",
  "author": "PeculiarVentures Team",
  "homepage": "https://x509.io/",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
