{
  "name": "@peculiar/certificates-viewer",
  "version": "1.0.1",
  "description": "Web components for viewing lists of certificates and certificates",
  "keywords": [
    "certificates-viewer",
    "certificate-viewer",
    "certificate",
    "decoding",
    "web-component",
    "webpki",
    "pki",
    "asn1"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/@peculiar/certificates-viewer/peculiar.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "npm run clean && stencil build --docs",
    "start": "npm run clean && stencil build --dev --watch --serve",
    "generate": "stencil generate",
    "deploy.gh-pages": "gh-pages -d www",
    "lint": "tslint --format stylish 'src/**/*.{ts,tsx}'",
    "clean": "rimraf dist loader www",
    "release": "standard-version",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@stencil/core": "^1.16.2",
    "@stencil/sass": "^1.3.2",
    "gh-pages": "^2.2.0",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.1",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-stencil": "^1.0.1"
  },
  "dependencies": {
    "@peculiar/asn1-adobe-acrobat": "^2.0.10",
    "@peculiar/asn1-cert-transparency": "^2.0.8",
    "@peculiar/asn1-ecc": "^2.0.8",
    "@peculiar/asn1-lei": "^2.0.8",
    "@peculiar/asn1-rsa": "^2.0.10",
    "@peculiar/asn1-schema": "^2.0.8",
    "@peculiar/asn1-x509": "^2.0.10",
    "@peculiar/asn1-x509-microsoft": "^2.0.10",
    "@peculiar/asn1-x509-netscape": "^2.0.8",
    "@peculiar/asn1-x509-qualified": "^2.0.10",
    "dayjs": "^1.8.29",
    "history": "^4.10.1",
    "pvtsutils": "^1.0.10"
  },
  "license": "MIT",
  "author": "PeculiarVentures Team",
  "homepage": "https://understandingwebpki.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeculiarVentures/pv-certificates-viewer.git"
  }
}
